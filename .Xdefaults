Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.antialias: 1
Xft.dpi: 96

!emacs.font: -unknown-Inconsolata-normal-normal-normal-*-14-*-*-*-m-0-iso10646-1

! from http://git.sysphere.org/dotfiles/tree/Xdefaults
!{{{ URXVT
!
! General
URxvt*termName:           rxvt-unicode-256color
URxvt*loginShell:         true
URxvt*scrollBar:         false
URxvt*secondaryScroll:    true
URxvt*saveLines:         65535
URxvt*cursorBlink:       false
!URxvt*cursorUnderline:   true
URxvt*urgentOnBell:       true
!URxvt*override-redirect:false
!URxvt*borderLess:       false
!URxvt*internalBorder:       0
!URxvt*externalBorder:       0
!
! Extensions
URxvt*perl-lib:        /usr/lib/urxvt/perl/
URxvt*perl-ext-common: default,matcher,searchable-scrollback
URxvt*urlLauncher:     /usr/bin/firefox
URxvt*matcher.button:  1
! Catch ugly URLs
URxvt*cutchars:        '"'()*<>[]{|}
!
! Appearance
!URxvt*font:     -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-iso8859-2
!`URxvt*boldFont: -xos4-terminus-bold-*-*-*-12-*-*-*-*-*-iso8859-2
!URxvt*font:     -misc-inconsolata-medium-r-normal--0-0-0-0-p-0-iso8859-2
!
!URxvt*font:  xft:Inconsolata:size=14
URxvt*font:   xft:Fira Code:style=Regular:size=14
! Cursor
URxvt*cursorColor: #DCDCCC
! URLs
URxvt*.colorUL:    #ffffff
!
! Pseudo Transparency
!URxvt*shading:    50
!URxvt*transparent:true
!
! Zenburn color-scheme
! URxvt*background:  #3f3f3f
! URxvt*foreground:  #dcdccc
! ! black + red
! !URxvt*color0:     #3f3f3f
